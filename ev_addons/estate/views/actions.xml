<odoo>
    <record id="action_estate_property" model="ir.actions.act_window">
        <field name="name">Estate Property</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">list,form</field>
        <field name="context">{'search_default_new': True}</field>
    </record>

    <record id="action_users_estate_property" model="ir.actions.act_window">
        <field name="name">Users Estate Property</field>
        <field name="res_model">estate.property</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('salesperson_id.id','=', active_id)]</field>
    </record>

    <record id="action_estate_type" model="ir.actions.act_window">
        <field name="name">Estate Type</field>
        <field name="res_model">estate.type</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_estate_tags" model="ir.actions.act_window">
        <field name="name">Estate Tags</field>
        <field name="res_model">estate.tags</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_estate_offer" model="ir.actions.act_window">
        <field name="name">Estate Offer</field>
        <field name="res_model">estate.offer</field>
        <field name="view_mode">list,form</field>
        <field name="domain">[('property_id', '=', active_id)]</field>
    </record>


</odoo>